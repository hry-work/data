config:
  # command被执行的目录，即R工程的工作目录

nodes:
  
  - name: start
    type: noop
    
  - name: noop_start@0300
    type: command
    dependsOn:
      - start
    config:
      command: /usr/lib64/R/bin/Rscript /root/data/flow/noop/noop_start.r -n '03:00'

  - name: dm_phone_property_related
    type: command
    dependsOn:
      - noop_start@0300
    config:
      command: /usr/lib64/R/bin/Rscript /root/data/dm/phone/dm_phone_property_related.r ${PARAMS}
      
  - name: dm_phone_water
    type: command
    dependsOn:
      - dm_phone_property_related
    config:
      command: /usr/lib64/R/bin/Rscript /root/data/dm/phone/dm_phone_water.r ${PARAMS}
      
  - name: dm_phone_electric
    type: command
    dependsOn:
      - dm_phone_property_related
    config:
      command: /usr/lib64/R/bin/Rscript /root/data/dm/phone/dm_phone_electric.r ${PARAMS}